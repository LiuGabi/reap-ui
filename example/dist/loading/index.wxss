.reap-loading{z-index:0;line-height:0;position:relative;display:inline-block;vertical-align:middle}.reap-loading-spinner{z-index:-1;width:100%;height:100%;position:relative;display:inline-block;box-sizing:border-box;animation:reap-rotate .8s linear infinite}.reap-loading-spinner-spinner{animation-timing-function:steps(12)}.reap-loading-spinner-circular{border:1px solid;border-radius:100%;border-color:transparent;border-top-color:currentColor}.reap-loading__dot{top:0;left:0;width:100%;height:100%;position:absolute}.reap-loading__dot::before{width:2px;height:25%;content:' ';display:block;margin:0 auto;border-radius:40%;background-color:currentColor}.reap-loading-dot:nth-of-type(1){opacity:1;transform:rotate(30deg)}.reap-loading-dot:nth-of-type(2){opacity:.9375;transform:rotate(60deg)}.reap-loading-dot:nth-of-type(3){opacity:.875;transform:rotate(90deg)}.reap-loading-dot:nth-of-type(4){opacity:.8125;transform:rotate(120deg)}.reap-loading-dot:nth-of-type(5){opacity:.75;transform:rotate(150deg)}.reap-loading-dot:nth-of-type(6){opacity:.6875;transform:rotate(180deg)}.reap-loading-dot:nth-of-type(7){opacity:.625;transform:rotate(210deg)}.reap-loading-dot:nth-of-type(8){opacity:.5625;transform:rotate(240deg)}.reap-loading-dot:nth-of-type(9){opacity:.5;transform:rotate(270deg)}.reap-loading-dot:nth-of-type(10){opacity:.4375;transform:rotate(300deg)}.reap-loading-dot:nth-of-type(11){opacity:.375;transform:rotate(330deg)}.reap-loading-dot:nth-of-type(12){opacity:.3125;transform:rotate(360deg)}@keyframes reap-rotate{from{transform:rotate(0)}to{transform:rotate(360deg)}}